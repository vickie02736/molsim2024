metad: METAD ARG=cv.x,cv.y SIGMA=0.1,0.1 HEIGHT=0.1 PACE=500 FILE=HILLS

PRINT ARG=cv.x,cv.y,metad.bias STRIDE=100 FILE=colvar.out






